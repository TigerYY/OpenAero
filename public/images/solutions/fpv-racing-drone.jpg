<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景 -->
  <rect width="400" height="300" fill="#1a1a2e"/>
  
  <!-- 赛道背景 -->
  <rect x="0" y="200" width="400" height="100" fill="#16213e"/>
  <rect x="0" y="220" width="400" height="5" fill="#e94560"/>
  <rect x="0" y="240" width="400" height="5" fill="#e94560"/>
  <rect x="0" y="260" width="400" height="5" fill="#e94560"/>
  
  <!-- 无人机主体 -->
  <g transform="translate(200,100)">
    <!-- 中央机身 (更紧凑的设计) -->
    <rect x="-20" y="-8" width="40" height="16" fill="#0f3460" rx="3"/>
    
    <!-- 螺旋桨臂 (X型布局) -->
    <line x1="-35" y1="-25" x2="35" y2="25" stroke="#e94560" stroke-width="3"/>
    <line x1="-35" y1="25" x2="35" y2="-25" stroke="#e94560" stroke-width="3"/>
    
    <!-- 电机和螺旋桨 -->
    <g transform="translate(-35,-25)">
      <circle cx="0" cy="0" r="8" fill="#533483"/>
      <circle cx="0" cy="0" r="15" fill="#e94560" opacity="0.3"/>
      <line x1="-12" y1="0" x2="12" y2="0" stroke="#e94560" stroke-width="1"/>
      <line x1="0" y1="-12" x2="0" y2="12" stroke="#e94560" stroke-width="1"/>
    </g>
    <g transform="translate(35,-25)">
      <circle cx="0" cy="0" r="8" fill="#533483"/>
      <circle cx="0" cy="0" r="15" fill="#e94560" opacity="0.3"/>
      <line x1="-12" y1="0" x2="12" y2="0" stroke="#e94560" stroke-width="1"/>
      <line x1="0" y1="-12" x2="0" y2="12" stroke="#e94560" stroke-width="1"/>
    </g>
    <g transform="translate(-35,25)">
      <circle cx="0" cy="0" r="8" fill="#533483"/>
      <circle cx="0" cy="0" r="15" fill="#e94560" opacity="0.3"/>
      <line x1="-12" y1="0" x2="12" y2="0" stroke="#e94560" stroke-width="1"/>
      <line x1="0" y1="-12" x2="0" y2="12" stroke="#e94560" stroke-width="1"/>
    </g>
    <g transform="translate(35,25)">
      <circle cx="0" cy="0" r="8" fill="#533483"/>
      <circle cx="0" cy="0" r="15" fill="#e94560" opacity="0.3"/>
      <line x1="-12" y1="0" x2="12" y2="0" stroke="#e94560" stroke-width="1"/>
      <line x1="0" y1="-12" x2="0" y2="12" stroke="#e94560" stroke-width="1"/>
    </g>
    
    <!-- FPV摄像头 -->
    <rect x="-8" y="-15" width="16" height="8" fill="#f5f5f5" rx="2"/>
    <circle cx="0" cy="-11" r="4" fill="#2c2c2c"/>
    <circle cx="0" cy="-11" r="2" fill="#4a90e2"/>
    
    <!-- LED灯带 -->
    <rect x="-18" y="10" width="36" height="3" fill="#00ff88" rx="1"/>
    <circle cx="-15" cy="11.5" r="1" fill="#00ff88"/>
    <circle cx="-5" cy="11.5" r="1" fill="#00ff88"/>
    <circle cx="5" cy="11.5" r="1" fill="#00ff88"/>
    <circle cx="15" cy="11.5" r="1" fill="#00ff88"/>
    
    <!-- 天线 -->
    <line x1="-25" y1="-5" x2="-35" y2="-15" stroke="#ffd700" stroke-width="2"/>
    <line x1="25" y1="-5" x2="35" y2="-15" stroke="#ffd700" stroke-width="2"/>
    <circle cx="-35" cy="-15" r="2" fill="#ffd700"/>
    <circle cx="35" cy="-15" r="2" fill="#ffd700"/>
  </g>
  
  <!-- 速度线条效果 -->
  <g opacity="0.6">
    <line x1="50" y1="80" x2="20" y2="85" stroke="#e94560" stroke-width="2"/>
    <line x1="60" y1="100" x2="30" y2="105" stroke="#e94560" stroke-width="2"/>
    <line x1="55" y1="120" x2="25" y2="125" stroke="#e94560" stroke-width="2"/>
    <line x1="350" y1="90" x2="380" y2="85" stroke="#e94560" stroke-width="2"/>
    <line x1="340" y1="110" x2="370" y2="105" stroke="#e94560" stroke-width="2"/>
    <line x1="345" y1="130" x2="375" y2="125" stroke="#e94560" stroke-width="2"/>
  </g>
  
  <!-- 赛道标记 -->
  <g fill="#ffd700">
    <polygon points="50,200 60,200 55,190" />
    <polygon points="150,200 160,200 155,190" />
    <polygon points="250,200 260,200 255,190" />
    <polygon points="350,200 360,200 355,190" />
  </g>
  
  <!-- 标题文字 -->
  <text x="200" y="40" text-anchor="middle" fill="#e94560" font-size="18" font-family="Arial, sans-serif" font-weight="bold">FPV竞速无人机套件</text>
  <text x="200" y="60" text-anchor="middle" fill="#ffd700" font-size="12" font-family="Arial, sans-serif">高速飞行 · 第一人称视角 · 竞技专用</text>
</svg>