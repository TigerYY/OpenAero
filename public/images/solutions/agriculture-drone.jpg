<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景 -->
  <rect width="400" height="300" fill="#e8f5e8"/>
  
  <!-- 农田背景 -->
  <rect x="0" y="200" width="400" height="100" fill="#90c695"/>
  <rect x="0" y="220" width="400" height="80" fill="#7bb87f"/>
  
  <!-- 无人机主体 -->
  <g transform="translate(200,120)">
    <!-- 中央机身 -->
    <ellipse cx="0" cy="0" rx="30" ry="15" fill="#2c3e50"/>
    
    <!-- 螺旋桨臂 -->
    <line x1="-40" y1="-20" x2="40" y2="20" stroke="#34495e" stroke-width="4"/>
    <line x1="-40" y1="20" x2="40" y2="-20" stroke="#34495e" stroke-width="4"/>
    
    <!-- 螺旋桨 -->
    <g transform="translate(-40,-20)">
      <circle cx="0" cy="0" r="12" fill="#3498db" opacity="0.7"/>
      <line x1="-10" y1="0" x2="10" y2="0" stroke="#2980b9" stroke-width="2"/>
      <line x1="0" y1="-10" x2="0" y2="10" stroke="#2980b9" stroke-width="2"/>
    </g>
    <g transform="translate(40,-20)">
      <circle cx="0" cy="0" r="12" fill="#3498db" opacity="0.7"/>
      <line x1="-10" y1="0" x2="10" y2="0" stroke="#2980b9" stroke-width="2"/>
      <line x1="0" y1="-10" x2="0" y2="10" stroke="#2980b9" stroke-width="2"/>
    </g>
    <g transform="translate(-40,20)">
      <circle cx="0" cy="0" r="12" fill="#3498db" opacity="0.7"/>
      <line x1="-10" y1="0" x2="10" y2="0" stroke="#2980b9" stroke-width="2"/>
      <line x1="0" y1="-10" x2="0" y2="10" stroke="#2980b9" stroke-width="2"/>
    </g>
    <g transform="translate(40,20)">
      <circle cx="0" cy="0" r="12" fill="#3498db" opacity="0.7"/>
      <line x1="-10" y1="0" x2="10" y2="0" stroke="#2980b9" stroke-width="2"/>
      <line x1="0" y1="-10" x2="0" y2="10" stroke="#2980b9" stroke-width="2"/>
    </g>
    
    <!-- 喷洒系统 -->
    <rect x="-15" y="15" width="30" height="10" fill="#e74c3c" rx="5"/>
    <text x="0" y="22" text-anchor="middle" fill="white" font-size="8" font-family="Arial">10L</text>
    
    <!-- 喷洒效果 -->
    <g opacity="0.6">
      <circle cx="-20" cy="35" r="3" fill="#27ae60"/>
      <circle cx="-10" cy="40" r="2" fill="#27ae60"/>
      <circle cx="0" cy="38" r="3" fill="#27ae60"/>
      <circle cx="10" cy="42" r="2" fill="#27ae60"/>
      <circle cx="20" cy="36" r="3" fill="#27ae60"/>
    </g>
    
    <!-- GPS/RTK模块 -->
    <rect x="-5" y="-25" width="10" height="8" fill="#f39c12" rx="2"/>
    <circle cx="0" cy="-21" r="2" fill="#e67e22"/>
  </g>
  
  <!-- 农作物 -->
  <g fill="#2ecc71">
    <rect x="50" y="240" width="3" height="20"/>
    <rect x="80" y="235" width="3" height="25"/>
    <rect x="110" y="238" width="3" height="22"/>
    <rect x="140" y="242" width="3" height="18"/>
    <rect x="170" y="236" width="3" height="24"/>
    <rect x="200" y="240" width="3" height="20"/>
    <rect x="230" y="238" width="3" height="22"/>
    <rect x="260" y="235" width="3" height="25"/>
    <rect x="290" y="242" width="3" height="18"/>
    <rect x="320" y="237" width="3" height="23"/>
  </g>
  
  <!-- 标题文字 -->
  <text x="200" y="40" text-anchor="middle" fill="#2c3e50" font-size="18" font-family="Arial, sans-serif" font-weight="bold">农业植保无人机</text>
  <text x="200" y="60" text-anchor="middle" fill="#7f8c8d" font-size="12" font-family="Arial, sans-serif">大载重 · 精准喷洒 · RTK定位</text>
</svg>